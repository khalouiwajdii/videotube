{% extends 'base.html.twig' %}

{% block title %}Search for {{ search }}{% endblock %}

{% block body %}
<h1 class="my-4">
    Search for <em>{{ search }}</em>
</h1>

<div class="row gy-4">
    {% for video in videos %}
    <div class="col-md-6 col-xl-4">
        <div class="card h-100">
            {{ include('cards/video.html.twig') }}
        </div>
    </div>
    {% endfor %}
</div>
{% endblock %}
